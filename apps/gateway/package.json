{
  "name": "@mcprism/gateway",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun run dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run src/db/migrate.ts"
  },
  "dependencies": {
    "elysia": "^1.1.29",
    "@xenova/transformers": "^2.17.2",
    "drizzle-orm": "^0.36.4",
    "postgres": "^3.4.5",
    "@mcprism/db": "workspace:*",
    "@mcprism/types": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.29.1"
  }
}
